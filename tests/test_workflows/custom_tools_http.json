{
    "config": {
        "max_steps": 50,
        "max_time": 200,
        "tools": [
            "ALL"
        ],
        "custom_tools": [
            {
                "name": "PriceFeedRequest",
                "description": "Fetches price feed from Gemini API",
                "mode": "http_request",
                "url": "https://api.gemini.com/v1/pricefeed",
                "method": "GET"
            }
        ]
    },
    "external_memory": {},
    "tasks": [
        {
            "id": "get_prices",
            "name": "Task",
            "description": "Task Description",
            "messages": [{"role": "user", "content":"What is the current BTC/USDT parity?"}],
            "inputs": [],
            "operator": "function_calling",
            "outputs": [
                {
                    "type": "write",
                    "key": "prices",
                    "value": "__result"
                }
            ]
        },
        {
            "id": "_end",
            "name": "Task",
            "description": "Task Description",
            "messages": [{"role": "user", "content": "End of the task"}],
            "inputs": [],
            "operator": "end",
            "outputs": []
        }
    ],
    "steps": [
        {
            "source": "get_prices",
            "target": "_end"
        }
    ],
    "return_value": {
        "input": {
            "type": "read",
            "key": "prices"
        },
        "to_json": false
    }
}