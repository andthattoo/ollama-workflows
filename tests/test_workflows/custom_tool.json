{
    "name": "Custom Tool",
    "description": "This is a simple workflow for custom tools",
    "config":{
        "max_steps": 5,
        "max_time": 100,
        "max_tokens": 1024,
        "tools": [],
        "custom_tools":[{
            "name": "google_search_tool",
            "description": "Conducts a web search using a specified search type and returns the results.",
            "mode":"custom",
            "parameters":{
                "type": "object",
                "properties": {
                    "website": {
                        "type": "string",
                        "description": "The URL of the website to scrape"
                    }
                },
                "required": ["website"]
            }
          }]
    },
    "tasks":[
        {
            "id": "A",
            "name": "Get prices",
            "description": "Get price feed",
            "prompt": "What are the current prices for $APPL?",
            "inputs":[],
            "operator": "function_calling_raw",
            "outputs":[
                {
                    "type": "write",
                    "key": "prices",
                    "value": "__result"
                }
            ]
        },
        {
            "id": "__end",
            "name": "end",
            "description": "End of the task",
            "prompt": "End of the task",
            "inputs": [],
            "operator": "end",
            "outputs": []
        }
    ],
    "steps":[
        {
            "source":"A",
            "target":"_end"
        }
    ],
    "return_value":{
        "input":{
            "type": "read",
            "key": "prices"
        }
    }
    }